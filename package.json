{
    "name": "twitter-clone",
    "version": "1.0.0",
    "description": "A Twitter clone application using modern web technologies",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "start": "node index.js",
        "dev": "nodemon index.js",
        "test": "jest",
        "lint": "eslint .",
        "client": "cd react && npm start",
        "server": "nodemon index.js",
        "dev:full": "concurrently \"npm run server\" \"npm run client\"",
        "build": "cd react && npm run build",
        "storybook": "cd react && npm run storybook",
        "test:e2e": "cypress run",
        "docker:up": "docker-compose up -d",
        "docker:down": "docker-compose down"
    },
    "dependencies": {
        "@eslint/js": "^9.9.0",
        "amqplib": "^0.10.0",
        "apollo-server-express": "^3.5.0",
        "bcrypt": "^5.1.1",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^16.0.0",
        "express": "^4.17.1",
        "express-rate-limit": "^7.4.0",
        "graphql": "^16.0.1",
        "ioredis": "^5.4.1",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^6.0.12",
        "redis": "^4.0.0",
        "socket.io": "^4.3.1",
        "ws": "^8.18.0"
    },
    "devDependencies": {
        "concurrently": "^7.0.0",
        "cypress": "^9.1.0",
        "docker-compose": "^0.23.13",
        "eslint": "^8.1.0",
        "jest": "^27.3.1",
        "nodemon": "^2.0.14",
        "supertest": "^6.1.6"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/yourusername/twitter-clone.git"
    },
    "keywords": [
        "twitter",
        "clone",
        "react",
        "nodejs",
        "express",
        "mongodb",
        "graphql",
        "apollo",
        "redux",
        "material-ui",
        "styled-components",
        "websockets"
    ],
    "author": "Your Name",
    "license": "MIT"
}
